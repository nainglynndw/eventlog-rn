{
  "name": "eventlog-rn",
  "version": "0.3.4",
  "description": "Functional, type-safe event logging SDK for React Native",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./viewer": {
      "types": "./dist/viewer/index.d.ts",
      "import": "./dist/viewer/index.mjs",
      "require": "./dist/viewer/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "!**/__tests__",
    "!**/*.test.ts"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "react-native",
    "event-logging",
    "analytics",
    "local-first",
    "debugging",
    "functional-programming"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react-native": ">=0.70.0",
    "react-native-mmkv": ">=3.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.8",
    "@types/react-native": "^0.70.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitepress": "^1.6.4"
  },
  "dependencies": {}
}
